
blue         = "\033[94m"
cyan         = "\033[96m"
green        = "\033[32m"
red          = "\033[91m"
orange       = "\033[33m"
yellow       = "\033[33m"
white        = "\033[97m"
grey         = "\033[90m"
bold         = "\033[1m"
reset_format = "\033[0m"

yesno = f"[{bold}{green}Yes{reset_format}/{bold}{red}No{reset_format}]"
noyes = f"[{bold}{red}Yes{reset_format}/{bold}{green}No{reset_format}]"
